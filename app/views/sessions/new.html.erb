Log In

<%= form_tag sessions_path do %>
  <%= text_field_tag :email, params[:email] %>
  <%= password_field_tag :password, params[:password] %>
  <%= link_to "Forgot Password?", password_reset_path %>
  <%= check_box_tag :remember_me, 1, params[:remember_me] %>
  <%= submit_tag "Log In" %>
<% end %>
